{
  "name": "virtual-cards",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "firebase emulators:exec \"vue-cli-service serve\"",
    "build": "vue-cli-service build",
    "ci:functions": "cd functions && npm ci",
    "build:functions": "cd functions && npm run build",
    "deploy": "npm run build && firebase deploy",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "firebase emulators:exec \"vue-cli-service test:e2e --headless\"",
    "test:all": "npm run test:unit && npm run test:e2e",
    "lint": "vue-cli-service lint",
    "test:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --no-cache --watch --runInBand"
  },
