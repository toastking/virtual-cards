{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "ban-types": false,
    "prettier": true
  }
}
